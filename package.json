{
  "name": "@versett/stylelint-config-versett",
  "version": "0.0.0-semantically-released",
  "description": "A plugin helping linitng styles",
  "main": "index.js",
  "scripts": {
    "precommit": "versett-devtools precommit",
    "preparecommitmsg": "versett-devtools preparecommitmsg ${GIT_PARAMS}",
    "release": "yarn && versett-devtools release --npm-publish",
    "test": "jest",
    "test:ci": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versett/stylelint-plugin-versett.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/versett/stylelint-plugin-versett/issues"
  },
  "homepage": "https://github.com/versett/stylelint-plugin-versett#readme",
  "devDependencies": {
    "@versett/versett-devtools": "^1.9.0",
    "jest": "^23.6.0"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "getLastRelease": "last-release-git"
  },
  "peerDependencies": {
    "stylelint": "^9.10.1"
  }
}
